{
  "name": "transform-string-case",
  "description": "Transform strings between snake, camel, and upper camel case.",
  "version": "0.0.0-development",
  "author": "Trezy <tre@trezy.com> (https://trezy.com)",
  "license": "BSD-3-Clause",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "build-coverage": "rollup dist/index.js --file coverage/index.js --format umd",
    "coverage": "yarn build-coverage && yarn instrument-coverage && yarn generate-coverage | coveralls",
    "generate-coverage": "mocha coverage --recursive --reporter mocha-lcov-reporter",
    "instrument-coverage": "jscoverage dist coverage",
    "semantic-release": "semantic-release",
    "test": "mocha --recursive --require @babel/register"
  },
  "keywords": [
    "camelcase",
    "camelize",
    "capitalize",
    "case",
    "changecase",
    "convert",
    "first",
    "humanize",
    "letter",
    "lettercase",
    "lowercase",
    "pretty",
    "snakecase",
    "string",
    "text",
    "titleize",
    "transform",
    "transformcase",
    "underscore",
    "uppercamelcase",
    "uppercamelize",
    "uppercase"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "@fuelrats/eslint-config": "^2.0.0",
    "all-contributors-cli": "^6.8.0",
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "coveralls": "^3.0.5",
    "eslint": "^6.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.0",
    "jscoverage": "^0.6.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-notify": "^1.1.0",
    "rollup-plugin-progress": "^1.1.1",
    "rollup-plugin-size-snapshot": "^0.10.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-visualizer": "^2.5.0",
    "semantic-release": "^15.13.18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trezy-studios/transform-string-case.git"
  }
}
